Vagrant.configure("2") do |config|
    config.vm.box = "ubuntu/focal64"
  
    config.vm.provider "virtualbox" do |vb|
      vb.memory = "2048"
      vb.cpus = 2
    end
  
    config.vm.provision "shell", inline: <<-SHELL
    export AWS_ACCESS_KEY_ID="AKIAXYKJVQUW5KC6RTUY"
    export AWS_SECRET_ACCESS_KEY="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"
    SHELL
    config.vm.provision "shell", path: "./scripts/setup-ubuntu.sh"
  
    # Sync local project directory to the VM
    config.vm.synced_folder "../", "/home/vagrant/cloud1"
  end